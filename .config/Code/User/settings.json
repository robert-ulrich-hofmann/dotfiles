    // ZEN
    "zenMode.fullScreen": true,
    "zenMode.centerLayout": true,
    "zenMode.showTabs": "multiple",
    "zenMode.hideLineNumbers": false,
    "zenMode.hideStatusBar": true,
    "zenMode.hideActivityBar": true,
    "zenMode.silentNotifications": false,
    "zenMode.restore": false,

    /**
     * LANGUAGE SETTINGS
     */

    "html.format.enable": false,
    "html.autoClosingTags": false,

    "javascript.updateImportsOnFileMove.enabled": "never",

    "python.analysis.reportExtraTelemetry": false,
    "python.analysis.autoIndent": true,

    "typescript.updateImportsOnFileMove.enabled": "never",

    /**
     * LANGUAGE-SPECIFIC OVERRIDES
     */

    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools",
        "editor.semanticHighlighting.enabled": true,
        "editor.stickyScroll.defaultModel": "foldingProviderModel",
        "editor.suggest.insertMode": "replace"
    },

    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools",
        "editor.semanticHighlighting.enabled": true,
        "editor.stickyScroll.defaultModel": "foldingProviderModel",
        "editor.suggest.insertMode": "replace"
    },

    "[dockercompose]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-containers",
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        }
    },

    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-containers",
        "editor.quickSuggestions": {
            "strings": true
        }
    },

    "[go]": {
        "editor.defaultFormatter": "golang.go",
        "editor.insertSpaces": false,
        "editor.tabSize": 4,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },

    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.suggest.insertMode": "replace"
    },

    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    },

    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
        "editor.unicodeHighlight.ambiguousCharacters": false,
        "editor.unicodeHighlight.invisibleCharacters": false
    },

    "[plaintext]": {
        "editor.unicodeHighlight.ambiguousCharacters": false,
        "editor.unicodeHighlight.invisibleCharacters": false,
        "editor.wordBasedSuggestions": "currentDocument"
    },

    "[python]": {
        "editor.defaultFormatter": "ms-python.python",
        "editor.defaultColorDecorators": "never",
        "gitlens.codeLens.symbolScopes": ["!Module"]
    },

    "[shellscript]": {
        "files.eol": "\n",
        "editor.defaultColorDecorators": "auto"
    },

    "[sql]": {},

    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    "[xml]": {},

    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "keep",
        "editor.defaultColorDecorators": "never",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        }
    },

    /**
     * EXTENSIONS
     */

    // BETTER COMMENTS
    "better-comments.highlightPlainText": true,
    "better-comments.multilineComments": true,
    "better-comments.tags": [
        // most languages
        {
            "tag": "TODO",
            "color": "#ff8c00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        // HTML
        {
            "tag": "<!-- TODO",
            "color": "#ff8c00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        // pandoc (Markdown, TeX)
        {
            "tag": "<!--- TODO",
            "color": "#ff8c00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        }
    ],

    // DRAWIO
    "hediet.vscode-drawio.resizeImages": null,

    // EDITORCONFIG
    "editorconfig.generateAuto": true,
    "editorconfig.showMenuEntry": true,
    "editorconfig.template": "default",

    // ESLINT
    "eslint.enable": true,
    "eslint.format.enable": false,

    // GITLENS
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.defaultGravatarsStyle": "mp",
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true
    },
    "gitlens.telemetry.enabled": false,
    "gitlens.ai.enabled": false,

    // PRETTIER
    "prettier.enable": true,
    "prettier.printWidth": 80,
    "prettier.useTabs": false,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "none",

    // REDHAT
    "redhat.telemetry.enabled": false
}
